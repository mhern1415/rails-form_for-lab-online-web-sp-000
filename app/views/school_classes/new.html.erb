<h1>School Class Form</h1>
<%= form_for @school_classes do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  
  <%= f.label :room_number %>
  <%= f.text_field :room_number %>
  
  <%= f.submit %>


<h1>Student Form</h1>
<%= form_for @student do |f| %>
  <%= f.label :first_name %>:
  <%= f.text_field :first_name %><br />

  <%= f.label :last_name %>:
  <%= f.text_field :last_name %><br />

  <%= f.submit %>
<% end %>